{
  "scripts": {
    "start": "npm-run-all --parallel css eleventy browsersync",
    "eleventy": "eleventy --watch",
    "debug": "set DEBUG=* & eleventy",
    "css": "postcss src/static/css/styles.css --o _site/static/css/styles.css --watch",
    "build": "cross-env NODE_ENV=production eleventy && cross-env NODE_ENV=production postcss src/static/css/styles.css --o _site/static/css/styles.css",
    "browsersync": "browser-sync start --server '_site' --files '_site' --port 8080 --no-notify --no-open",
    "cms": "npx netlify-cms-proxy-server"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.1",
    "autoprefixer": "^10.2.6",
    "browser-sync": "^2.26.14",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.6",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "luxon": "^1.27.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.2",
    "postcss-cli": "^8.3.1",
    "prismjs": "^1.23.0"
  },
  "dependencies": {}
}
